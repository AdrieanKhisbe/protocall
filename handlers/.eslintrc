{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "globals": {},
  "rules": {
    "no-debugger": "off",
    "strict": [
      "error",
      "global"
    ],
    "semi": "off",
    "linebreak-style": "off",
    "no-bitwise": "off",
    "no-cond-assign": [
      "error",
      "except-parens"
    ],
    "curly": "error",
    "eqeqeq": "error",
    "no-eq-null": "off",
    "no-eval": "off",
    "no-unused-expressions": "error",
    "guard-for-in": "off",
    "wrap-iife": [
      "error",
      "any"
    ],
    "no-use-before-define": "off",
    "no-loop-func": "off",
    "no-caller": "off",
    "no-script-url": "off",
    "no-shadow": "error",
    "no-new-func": "off",
    "no-new-wrappers": "off",
    "no-undef": "error",
    "no-invalid-this": "off",
    "no-proto": "off",
    "no-multi-str": "off",
    "no-iterator": "off",
    "block-scoped-var": "off",
    "new-cap": "off",
    "no-empty": "off",
    "no-new": "off",
    "no-plusplus": "off",
    "dot-notation": "error",
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "max-params": [
      "error",
      5
    ],
    "max-depth": [
      "error",
      3
    ],
    "max-statements": [
      "error",
      20
    ],
    "complexity": [
      "error",
      8
    ]
  }
}
